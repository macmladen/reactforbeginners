{
  "name": "reactforbeginners",
  "version": "0.0.1",
  "description": "The Catch-of-the-Day sample app from React for Beginners by Wes Bos.",
  "main": "index.js",
  "private": false,
  "devDependencies": {
    "concurrently": "6.4.0",
    "react-scripts": "4.0.3"
  },
  "dependencies": {
    "autoprefixer-stylus": "1.0.0",
    "firebase": "8.10.0",
    "prettier": "^2.7.1",
    "prop-types": "15.7.2",
    "re-base": "4.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-transition-group": "4.4.2",
    "serve": "13.0.2",
    "stylus": "0.55.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "now-build": "npm run build && rm -rf docs && mv build docs",
    "styles:watch": "npm run styles -- -w"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macmladen/react-for-beginners.git"
  },
  "keywords": [
    "React",
    "Wes",
    "Bos",
    "React",
    "for",
    "Beginners"
  ],
  "author": "Mladen Djuric @MacMladen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/macmladen/react-for-beginners/issues"
  }
}
